def pascal_triangle(n):
    for i in range(n):
        num = 1
        row = ""
        for j in range(i + 1):
            row += str(num)
            num = num * (i - j) // (j + 1)
        # Pad with trailing zeroes to make all rows the same length
        row = row.ljust(n, '0')
        print(row)

# Input from user
N = int(input("Enter number of rows: "))
pascal_triangle(N)
